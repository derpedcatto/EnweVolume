<Application
    x:Class="EnweVolume.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:EnweVolume"
    xmlns:localservices="clr-namespace:EnweVolume.Core.Services"
    xmlns:localview="clr-namespace:EnweVolume.MVVM.Views"
    ThemeMode="System">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/StringResources.en-US.xaml" />
                <ResourceDictionary Source="/Resources/StringResources.en-GB.xaml" />
                <ResourceDictionary Source="/Resources/StringResources.uk-UA.xaml" />
                <ResourceDictionary Source="/Resources/StringResources.ru-RU.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
