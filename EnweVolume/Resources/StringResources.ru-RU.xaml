<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!--  Window Title  -->
    <system:String x:Key="WindowTitle">Настройки EnweVolume</system:String>

    <!--  Group Headers  -->
    <system:String x:Key="Group_CurrentVolume">Текущая громкость</system:String>
    <system:String x:Key="Group_RedThreshold">Красный порог громкости</system:String>
    <system:String x:Key="Group_YellowThreshold">Желтый порог громкости</system:String>
    <system:String x:Key="Group_GeneralSettings">Общие настройки</system:String>

    <!--  Labels  -->
    <system:String x:Key="Label_Zero">0</system:String>
    <system:String x:Key="Label_Hundred">100</system:String>
    <system:String x:Key="Label_ChangeBarColor">Менять цвет индикатора громкости</system:String>
    <system:String x:Key="Label_VolumeThreshold">Порог громкости</system:String>
    <system:String x:Key="Label_Notifications">Уведомления</system:String>
    <system:String x:Key="Label_SendPushNotification">Отсылать Push-уведомления</system:String>
    <system:String x:Key="Label_PlaySoundNotification">Проигрывать звуковое уведомление</system:String>
    <system:String x:Key="Label_TestVolume">Проверка громкости</system:String>
    <system:String x:Key="Label_EnableYellowThreshold">Включить желтый порог громкости</system:String>
    <system:String x:Key="Label_StartWithSystem">Запускать с системой</system:String>
    <system:String x:Key="Label_CurrentAudioDevice">Текущее аудиоустройство</system:String>
    <system:String x:Key="Label_AppLocale">Язык приложения</system:String>
    <system:String x:Key="Label_ThemeSoundSettings">Настройки темы / звука</system:String>

    <!--  Error Messages  -->
    <system:String x:Key="Error_Unknown">Произошла неизвестная ошибка.</system:String>
    <!--  Error Messages: Validation  -->
    <system:String x:Key="Error_InvalidUserSettings">Некорректные настройки пользователя.</system:String>
    <!--  Error Messages: IO  -->
    <system:String x:Key="Error_UserSettingsSaveError">Не удалось сохранить настройки пользователя.</system:String>
    <system:String x:Key="Error_UserSettingsLoadError">Не удалось загрузить настройки пользователя.</system:String>
    <system:String x:Key="Error_SettingsFileCorrupted">Файл настроек поврежден.</system:String>
    <system:String x:Key="Error_SettingsFileLockError">Файл настроек заблокирован другим процессом.</system:String>
    <system:String x:Key="Error_SettingsDirectoryAccessError">Не удалось получить доступ к каталогу настроек.</system:String>
    <!--  Error Messages: Device  -->
    <system:String x:Key="Error_DeviceNotFound">Аудиоустройство не найдено.</system:String>
    <system:String x:Key="Error_DeviceAccessDenied">Доступ к аудиоустройству запрещен.</system:String>
    <system:String x:Key="Error_DeviceDisposed">Аудиоустройство было освобождено.</system:String>
    <!--  Error Messages: Runtime  -->
    <system:String x:Key="Error_OperationCanceled">Операция была отменена.</system:String>
    <system:String x:Key="Error_PermissionDenied">Отказано в доступе.</system:String>

    <!--  Other  -->
    <system:String x:Key="String_DefaultAudioDevice">По умолчанию</system:String>
</ResourceDictionary>